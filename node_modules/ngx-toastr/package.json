{
  "name": "ngx-toastr",
  "version": "16.0.1",
  "peerDependencies": {
    "@angular/core": ">=14.0.0-0",
    "@angular/common": ">=14.0.0-0",
    "@angular/platform-browser": ">=14.0.0-0"
  },
  "exports": {
    "./toastr": {
      "default": "./toastr.css"
    },
    "./toastr-old": {
      "default": "./toastr-old.css"
    },
    "./toastr-bs4-alert": {
      "default": "./toastr-bs4-alert.scss"
    },
    "./toastr-bs5-alert": {
      "default": "./toastr-bs5-alert.scss"
    },
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ngx-toastr.mjs",
      "es2020": "./fesm2020/ngx-toastr.mjs",

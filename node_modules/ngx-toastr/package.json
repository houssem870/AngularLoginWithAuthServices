  "module": "fesm2015/ngx-toastr.mjs",
  "es2020": "fesm2020/ngx-toastr.mjs",
  "esm2020": "esm2020/ngx-toastr.mjs",
  "fesm2020": "fesm2020/ngx-toastr.mjs",
  "fesm2015": "fesm2015/ngx-toastr.mjs",
